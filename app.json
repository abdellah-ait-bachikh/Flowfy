{
  "expo": {
    "name": "Flowfy",
    "slug": "Flowfy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "flowfy10",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abdellahbachikh.Flowfy",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "icon": "./assets/images/flowfy-border-r-removed.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.abdellahbachikh.Flowfy",
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#fff200"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 300,
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eef1743d-c01c-44bb-8093-602360af2a07"
      }
    },
    "owner": "abdellah_ait_bachikh"
  }
}
